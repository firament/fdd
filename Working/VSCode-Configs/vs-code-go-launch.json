//
// Replace '/YOUR/PROJECT/PATH' with the your project path
// e.g. '/01-Work/gits/project-abc'
//
// original file name: /YOUR/PROJECT/PATH/.vscode/launch.json
{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch GO Debugger",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/src/main.go",
            "env": {
                "GOPATH": "/10-Base/go-package-lib:/YOUR/PROJECT/PATH",
                "GOROOT": "/10-Base/go",
                "TOOLSGOPATH": "/10-Base/go-tools",
            },
			"args": [],
			"showLog": false
		}
	]
}
